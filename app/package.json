{
  "name": "smartadmin",
  "version": "0.4.5",
  "license": "WRAPBOOTSTRAP",
  "angular-cli": {},
  "scripts": {
    "build:dev": "npm run config -- --environment=dev && ./node_modules/.bin/ng build",
    "build:prod": "npm run config -- --environment=prod && ./node_modules/.bin/ng build --prod --aot=false",
    "build:aot": "npm run config -- --environment=prod && ./node_modules/.bin/ng build --prod --aot",
    "build:aot2": "npm run config -- --environment=prod && node --max_old_space_size=4096 ./node_modules/.bin/ng build --prod --aot",
    "build": "npm run build:dev",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean:start": "npm start",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "config": "./node_modules/.bin/ts-node ./scripts/set-env.ts",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "npm run protractor",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "postversion": "git push && git push --tags",
    "prebuild:dev": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist",
    "preclean:install": "npm run clean",
    "preclean:start": "npm run clean",
    "preversion": "npm test",
    "protractor": "protractor",
    "rimraf": "rimraf",
    "server:dev": "./node_modules/.bin/ng serve --host 0.0.0.0 --disable-host-check --proxy-config proxy.conf.json",
    "server": "npm run config -- --environment=dev && npm run server:dev",
    "start": "npm run config -- --environment=dev && npm run server:dev",
    "test": "./node_modules/.bin/ng test",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "npm run build",
    "ng": "ng",
    "pree2e": "webdriver-manager update --standalone false --gecko false"
  },
  "private": true,
  "dependencies": {
