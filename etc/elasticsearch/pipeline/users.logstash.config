input {
  file {
    path            => "/import_data/users.csv"
    start_position  => "beginning"
    sincedb_path    => "/dev/null"
  }
}
filter {
  csv {
    separator => ","
    columns => [ "id", "name", "username" ]
  }
}
output {
  elasticsearch {
    action => "index"
    hosts => [ "elasticsearch:9200" ]
    index => "users"
    document_type => "user"
    document_id => "%{id}"
  }
  stdout {}
}